{
  "name": "nezukochan",
  "version": "5.0.0",
  "description": "A best free to use discord bot, non premium music bot",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "KagChi"
  },
  "repository": {
    "url": "https://github.com/NezuChan/nezu"
  },
  "scripts": {
    "start:dev": "ts-node -r dotenv/config \"src/structures/NezuClient.ts\"",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "lint:fix": "eslint . --ignore-path .gitignore --ext .ts --fix",
    "start": "node -r dotenv/config dist",
    "compile": "tsc"
  },
  "dependencies": {
    "@discordjs/builders": "0.7.0",
    "@sapphire/decorators": "3.1.3",
    "@sapphire/discord.js-utilities": "3.2.2",
    "@sapphire/framework": "2.1.3",
    "@sapphire/plugin-logger": "2.0.1",
    "@sapphire/plugin-subcommands": "2.0.1",
    "@sapphire/utilities": "3.0.4",
    "@sentry/node": "6.13.3",
    "@sentry/tracing": "6.13.3",
    "@top-gg/sdk": "3.1.2",
    "bufferutil": "4.0.5",
    "cheerio": "1.0.0-rc.10",
    "colorette": "2.0.16",
    "discord-api-types": "0.24.0",
    "discord.js": "13.2.0",
    "dotenv": "10.0.0",
    "erlpack": "github:discord/erlpack",
    "js-yaml": "4.1.0",
    "mongodb": "3.7.3",
    "petitio": "1.4.0",
    "shoukaku": "2.1.0",
    "spotify-url-info": "2.2.3",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typeorm": "0.2.38",
    "utf-8-validate": "5.0.7",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "7.0.1",
    "@types/js-yaml": "4.0.4",
    "@types/ms": "0.7.31",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "sort-keys": 0,
      "@typescript-eslint/no-non-null-asserted-optional-chain": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/no-unnecessary-condition": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-extraneous-class": 0,
      "@typescript-eslint/member-ordering": 0,
      "@typescript-eslint/restrict-template-expressions": 0,
      "no-eq-null": 0,
      "no-negated-condition": 0,
      "@typescript-eslint/prefer-optional-chain": 0,
      "@typescript-eslint/no-unsafe-argument": 0,
      "@typescript-eslint/naming-convention": 0
    },
    "ignorePatterns": []
  }
}
